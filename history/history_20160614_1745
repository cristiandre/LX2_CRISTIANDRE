 1307  #!/bin/bash
 1308  ORIG=/home/sysadmin/lx2_cristiandre/problema2
 1309  DATA=/home/sysadmin/lx2_cristiandre/hojasExcel/hojasDatos
 1310  OUT_DATA=$DATA/datos_csv
 1311  GRAF_DATA=$DATA/datos_graf
 1312  mkdir $DATA/datos_csv
 1313  mkdir $GRAF_DATA
 1314  m=0
 1315  for i in `find $ORIG #name '*.xls' `; do echo "Procesando archivo $i"; xls2csv $i > $OUT_DATA/data-$m.csv; let m=m+1; done 2>/home/sysadmin/lx2_cristiandre/logs/error1.log
 1316  m=0
 1317  for e in `find $OUT_DATA #name "*.csv"`; do echo "Dando formato de datos para graficar el archivo $e"; cat $e | awk #F "\",\"" '{print $1 " " $2 " " $3 " " $4 " " $5}'| sed '1,$ s/"//g' | sed '1 s/date/#date/g' > $GRAF_DATA/graf-$m.dat;  let m=m+1;  done 2> /home/sysadmin/lx2_cristiandre/logs/error2.log
 1318  awk
 1319  sudo apt-get install awk
 1320  sudo apt-get install mawk
 1321  sudo apt-get install sed
 1322  sudo apt-get update
 1323  sudo apt-get upgrade
 1324  EXIT
